local Tab = Window:NewTab("Tech Passives(Iframes)")
local PSection = Tab:NewSection("TOGGLE KEYBIND")

PSection:NewButton("Limitless: Infinity (Combat)", "NO CD (LOOP)", function()
    local enabled = false
    game.UserInputService.InputBegan:Connect(function(input, gp)
        if gp then return end
        if input.KeyCode == Enum.KeyCode.Insert then
            enabled = not enabled
            if enabled then
                while wait(2) and enabled do
                        local args = {
            [1] = "Limitless: Infinity"
            }

            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Server"):WaitForChild("Combat"):WaitForChild("Skill"):FireServer(unpack(args))
                end
            end
        end
    end)
end)

PSection:NewButton("Infinity", "NO CD (LOOP)", function()
    local enabled = false
    game.UserInputService.InputBegan:Connect(function(input, gp)
        if gp then return end
        if input.KeyCode == Enum.KeyCode.Nine then
            enabled = not enabled
            if enabled then
                while wait(6) and enabled do
                        local args = {
            [1] = "Limitless: Infinity"
            }

            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Server"):WaitForChild("Combat"):WaitForChild("Skill"):FireServer(unpack(args))
                end
            end
        end
    end)
end)

PSection:NewButton("Super Senses (Combat)", "NO CD (LOOP)", function()
    local enabled = false
    game.UserInputService.InputBegan:Connect(function(input, gp)
        if gp then return end
        if input.KeyCode == Enum.KeyCode.Semicolon then
            enabled = not enabled
            if enabled then
                while wait(1) and enabled do
                        local args = {
            [1] = "Super Senses"
            }

            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Server"):WaitForChild("Combat"):WaitForChild("Skill"):FireServer(unpack(args))
                end
            end
        end
    end)
end)

