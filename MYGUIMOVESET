local Tab = Window:NewTab("TransFormation Buffs")
local TransformationsSection = Tab:NewSection("No CD, SOON MORE WILL BE ADDED")

TransformationsSection:NewButton("Divergent Fist Spam", "INF DAMAGE BUFFS", function()
    local enabled = false
    game.UserInputService.InputBegan:Connect(function(input, gp)
        if gp then return end
        if input.KeyCode == Enum.KeyCode.N then
            enabled = not enabled
            if enabled then
                while wait(0.1) and enabled do
                       local args = {
            [1] = "Divergent Fist: Activation"
            }
    
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Server"):WaitForChild("Combat"):WaitForChild("Skill"):FireServer(unpack(args))
                end
            end
        end
    end)
end)


